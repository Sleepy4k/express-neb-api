<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">

    <% if (block('subTitle').toString()) { %>
      <meta name="title" content="<%= title %> - <%= block('subTitle').toString() %>">
    <% } else { %>
      <meta name="title" content="<%= title %>">
    <% } %>

    <% if (typeof description !== 'undefined') { %>
      <meta name="description" content="<%= description %>">
    <% } %>

    <meta name="author" content="Benjamin4k">
    <meta name="coverage" content="Worldwide">
    <meta name="distribution" content="Global">
    <meta name="robots" content="noindex, nofollow">
    <meta name="keywords" content="seb, neb, bypass, seb bypass, neb bypass, seb bypasser, neb bypasser, seb bypass tool, neb bypass tool">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">

    <link rel="apple-touch-icon" href="<%= asset('favicon.ico') %>" />
    <link rel="icon" type="image/png" href="<%= asset('favicon.ico') %>" />
    <link rel="icon" type="image/x-icon" href="<%= asset('favicon.ico') %>" />
    <link rel="shortcut icon" type="image/png" href="<%= asset('favicon.ico') %>" />
    <link rel="shortcut icon" type="image/x-icon" href="<%= asset('favicon.ico') %>" />

    <% if (block('subTitle').toString()) { %>
      <title>
        <%= title %> - <%= block('subTitle').toString() %>
      </title>
    <% } else { %>
      <title><%= title %></title>
    <% } %>

    <meta property="csp-nonce" content="<%= cspNonce %>">

    <meta property="og:locale" content="en">
    <meta property="og:url" content="<%= baseUrl %>">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="<%= title %>">

    <% if (block('subTitle').toString()) { %>
      <meta property="og:title" content="<%= title %> - <%= block('subTitle').toString() %>">
    <% } else { %>
      <meta property="og:title" content="<%= title %>">
    <% } %>

    <% if (typeof description !== 'undefined') { %>
      <meta property="og:description" content="<%= description %>">
    <% } %>

    <meta property="og:image" content="<%= asset('images/seb.png') %>">
    <meta property="og:image:width" content="900">
    <meta property="og:image:height" content="549">

    <link rel="stylesheet" href="<%= asset('css/error.css') %>" nonce="<%= cspNonce %>" />
    <link rel="stylesheet" href="<%= asset('css/font.css') %>" nonce="<%= cspNonce %>" />

    <%- block('addon-style') %>
  </head>
  <body class="antialiased">
    <%- body %>

    <%- block('addon-script') %>
  </body>
</html>