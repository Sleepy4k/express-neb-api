<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">

    <% if (block('subTitle').toString()) { %>
      <meta name="title" content="<%= title %> - <%= block('subTitle').toString() %>">
    <% } else { %>
      <meta name="title" content="<%= title %>">
    <% } %>

    <% if (typeof description !== 'undefined') { %>
      <meta name="description" content="<%= description %>">
    <% } %>

    <meta name="author" content="Benjamin4k">
    <meta name="coverage" content="Worldwide">
    <meta name="distribution" content="Global">
    <meta name="robots" content="index, follow">
    <meta name="keywords" content="seb, neb, bypass, seb bypass, neb bypass, seb bypasser, neb bypasser, seb bypass tool, neb bypass tool">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">

    <link rel="canonical" href="<%= baseUrl %>" />
    <link rel="apple-touch-icon" href="<%= asset('logo.ico') %>" />
    <link rel="icon" type="image/png" href="<%= asset('logo.ico') %>" />
    <link rel="icon" type="image/x-icon" href="<%= asset('logo.ico') %>" />
    <link rel="shortcut icon" type="image/png" href="<%= asset('logo.ico') %>" />
    <link rel="shortcut icon" type="image/x-icon" href="<%= asset('logo.ico') %>" />

    <% if (block('subTitle').toString()) { %>
      <title>
        <%= title %> - <%= block('subTitle').toString() %>
      </title>
    <% } else { %>
      <title><%= title %></title>
    <% } %>

    <meta property="csp-nonce" content="<%= cspNonce %>">

    <meta property="og:locale" content="en">
    <meta property="og:url" content="<%= baseUrl %>">
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="<%= title %>">

    <% if (block('subTitle').toString()) { %>
      <meta property="og:title" content="<%= title %> - <%= block('subTitle').toString() %>">
    <% } else { %>
      <meta property="og:title" content="<%= title %>">
    <% } %>

    <% if (typeof description !== 'undefined') { %>
      <meta property="og:description" content="<%= description %>">
    <% } %>

    <meta property="og:image" content="<%= asset('logo.ico') %>">
    <meta property="og:image:width" content="900">
    <meta property="og:image:height" content="549">

    <link rel="stylesheet" href="<%= asset('vendor/css/bootstrap.min.css') %>" nonce="<%= cspNonce %>" />
    <link rel="stylesheet" href="<%= asset('vendor/css/bootstrap-icons.min.css') %>" nonce="<%= cspNonce %>" />
    <link rel="stylesheet" href="<%= asset('css/main.css') %>" nonce="<%= cspNonce %>" />

    <%- block('addon-style') %>
  </head>
  <body>
    <% if (isRouteActive() == 'active') { %>
      <div class="title">
        <%- partial('components/navbar') %>
        <%- block('hero') %>
      </div>
    <% } else { %>
      <%- partial('components/navbar') %>
    <% } %>

    <%- body %>

    <%- partial('components/footer') %>

    <script src="<%= asset('vendor/js/bootstrap.bundle.min.js') %>" nonce="<%= cspNonce %>"></script>

    <script src="<%= asset('js/main.js') %>" nonce="<%= cspNonce %>"></script>

    <script nonce="<%= cspNonce %>">
      document.getElementById("year").textContent = new Date().getFullYear();
    </script>

    <%- block('addon-script') %>
  </body>
</html>